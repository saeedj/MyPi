{
    "name": "laminas/laminas-test",
    "description": " ",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "test"
    ],
    "homepage": "https://laminas.dev",
    "support": {
        "docs": "https://docs.laminas.dev/laminas-test/",
        "issues": "https://github.com/laminas/laminas-test/issues",
        "source": "https://github.com/laminas/laminas-test",
        "rss": "https://github.com/laminas/laminas-test/releases.atom",
        "chat": "https://laminas.dev/chat",
        "forum": "https://discourse.laminas.dev"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.5-dev",
            "dev-develop": "2.6-dev"
        }
    },
    "require": {
        "php": ">=5.5",
        "laminas/laminas-console": "~2.5",
        "laminas/laminas-dom": "~2.5",
        "laminas/laminas-eventmanager": "~2.5",
        "laminas/laminas-http": "~2.5",
        "laminas/laminas-mvc": "~2.5",
        "laminas/laminas-servicemanager": "~2.5",
        "laminas/laminas-stdlib": "~2.5",
        "laminas/laminas-uri": "~2.5",
        "laminas/laminas-view": "~2.5",
        "laminas/laminas-zendframework-bridge": "^1.0",
        "phpunit/phpunit": "~4.0 || ~5.0"
    },
    "require-dev": {
        "fabpot/php-cs-fixer": "1.7.*",
        "laminas/laminas-i18n": "~2.5",
        "laminas/laminas-log": "~2.5",
        "laminas/laminas-modulemanager": "~2.5",
        "laminas/laminas-serializer": "~2.5",
        "laminas/laminas-session": "~2.5",
        "mikey179/vfsstream": "~1.2"
    },
    "autoload": {
        "psr-4": {
            "Laminas\\Test\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaminasTest\\Test\\": "test/"
        }
    },
    "replace": {
        "zendframework/zend-test": "self.version"
    }
}
